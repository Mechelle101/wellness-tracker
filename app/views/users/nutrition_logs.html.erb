<h1>Nutrition Logs for <%= @user.f_name %><%= @user.l_name %> </h1>

<% if @nutrition_logs.any? %>
<ul>
  <% @nutrition_logs.each do |log| %>
    <li>
      <strong><%= log.meal_type %></strong> - <%= log.food_type %>: <%= log.calories %> kcal
     (<%= log.protein_grm %>g protein, <%= log.carbs_grm %>g carbs, <%= log.fats_grm %>g fat)
    </li>
  <% end %>
</ul>
<% else %>
  <p>No nutrition logs found.</p>
<% end %>
